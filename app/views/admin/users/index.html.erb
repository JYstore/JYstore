<h1>Admin Users</h1>

<table class="table">
<tr>
  <th>ID</th>
  <th>Email</th>
  <th>注册时间</th>
  <th>操作选项</th>
</tr>
<% @users.each do |user| %>
  <tr>
    <td><%= user.id %></td>
    <td><%= user.email %></td>
    <td><%= user.created_at %></td>
    <td><%= link_to "编辑", edit_admin_user_path(user), :class => "btn btn-default" %></td>
  </tr>
<% end %>
</table>
